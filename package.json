{
	"name": "pure",
	"version": "1.0.0",
	"description": "try to made light performance first ui framework",
	"main": "index.js",
	"type": "module",
	"scripts": {
		"dev:client": "webpack-dev-server --config webpack.config.dev.js --open",
		"dev:server": "NODE_ENV=development node server/app.js",
		"dev": "NODE_ENV=development concurrently \"webpack-dev-server --config webpack.config.dev.js\" \"nodemon server/app.js\"",
		"build:dev": "NODE_ENV=development concurrently \"webpack --config webpack.config.build.js --env dev\" \"node server/app.js\"",
		"build:prod": "NODE_ENV=production webpack --config webpack.config.build.js --env production",
		"build-docker:dev": "NODE_ENV=development ./run-docker.sh",
		"build-docker:prod": "NODE_ENV=production ./run-docker.sh",
		"start": "NODE_ENV=production node server/app.js",
		"deploy": "node deploy",
		"test": "node test-geo-mapping.js && node test-auto-detection.js",
		"lint": "echo 'Add your linting command here'",
		"review": "cursor-agent --force --output-format=text --print 'Review the uncommitted changes in this repository. Focus on code quality, best practices, potential bugs, and adherence to project standards defined in .cursorrules. Provide concise, actionable feedback.'",
		"review:staged": "cursor-agent --force --output-format=text --print 'Review the staged changes in this repository. Focus on code quality, best practices, potential bugs, and adherence to project standards defined in .cursorrules. Provide concise, actionable feedback.'",
		"clean": "rm -rf dist",
		"prebuild": "npm run clean"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/shishiv30/pure.git"
	},
	"keywords": [
		"pure",
		"light",
		"fast",
		"uiframework"
	],
	"author": "conjee zou",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/shishiv30/pure/issues"
	},
	"homepage": "https://github.com/shishiv30/pure#readme",
	"nodemonConfig": {
		"ignore": [
			"dist",
			"node_modules"
		]
	},
	"devDependencies": {
		"babel-loader": "^9.1.0",
		"concurrently": "^9.2.0",
		"css-loader": "^6.7.3",
		"file-loader": "^6.2.0",
		"gh-pages": "^6.1.1",
		"html-loader": "^4.2.0",
		"html-webpack-plugin": "^5.5.0",
		"http-proxy-middleware": "^3.0.5",
		"lighthouse": "^9.0.0",
		"mini-css-extract-plugin": "^2.7.2",
		"sass": "^1.57.0",
		"sass-loader": "^13.2.0",
		"webpack": "^5.75.0",
		"webpack-bundle-analyzer": "^4.9.0",
		"webpack-cli": "^5.0.1",
		"webpack-dev-server": "^4.11.1",
		"webpack-merge": "^5.8.0",
		"webpack-pwa-manifest": "^4.0.0",
		"workbox-webpack-plugin": "^7.0.0"
	},
	"dependencies": {
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"csvtojson": "^2.0.10",
		"dotenv": "^16.5.0",
		"express-session": "^1.18.0",
		"express-useragent": "^1.0.15",
		"fuse.js": "^7.0.0",
		"geoip-lite": "^1.4.10",
		"lodash": "^4.17.21",
		"mime": "^4.0.7",
		"nodemon": "^3.1.4",
		"swagger-jsdoc": "^6.2.8",
		"swagger-ui-express": "^5.0.1",
		"uuid": "^9.0.0"
	}
}