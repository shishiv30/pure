<%if(breadcrumb && breadcrumb.links && breadcrumb.links.length){%>
  <nav class="breadcrumb gray" aria-label="breadcrumb">
    <ul>
      <% for(var i=0; i<breadcrumb.links.length; i++) { %>
          <li>
            <% if(i==breadcrumb.links.length - 1) { %>
              <%= breadcrumb.links[i].text %>
            <%} else {%>
                <a href="<%= breadcrumb.links[i].href %>">
                    <%= breadcrumb.links[i].text %>
                  </a>
              <%}%>
          </li>
        <%}%>
    </ul>
    <div class="breadcrumb-button">
      <a href="/sap.html" class="btn small">Back </a>
    </div>
  </nav>
  <%}%>